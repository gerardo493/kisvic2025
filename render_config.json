{
  "name": "mi-app-web",
  "type": "web",
  "env": "python",
  "plan": "free",
  "buildCommand": "pip install -r requirements_render.txt",
  "startCommand": "gunicorn app:app --bind 0.0.0.0:$PORT --workers 1 --timeout 120",
  "envVars": {
    "PYTHON_VERSION": "3.10.0",
    "FLASK_ENV": "production",
    "SECRET_KEY": "generate"
  },
  "requirements": {
    "python": "3.10.0",
    "flask": "2.2.5",
    "flask-wtf": "1.0.1",
    "gunicorn": "20.1.0"
  }
}